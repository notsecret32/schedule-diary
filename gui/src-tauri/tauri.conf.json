{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "schedule-diary",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": { "message": true },
      "fs": {
        "scope": ["$RESOURCE/*"]
      },
      "path": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "Schedule Diary",
        "fullscreen": false,
        "resizable": false,
        "width": 550,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.alexander.diary",
      "targets": "all"
    }
  }
}
